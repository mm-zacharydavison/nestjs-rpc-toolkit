{
  "name": "@meetsmore/lib-rpc",
  "version": "1.0.0",
  "description": "Package containing shared types for RPC calls in this monorepo (generated by @zdavison/nestjs-rpc-toolkit)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run generate:types && tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "generate:types": "ts-node scripts/generate-all-rpc-types.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/microservices": "^11.0.0",
    "@zdavison/nestjs-rpc-toolkit": "workspace:*",
    "@shared/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.5.1",
    "ts-morph": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
